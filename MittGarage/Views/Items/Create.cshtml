@model MittGarage.Models.GarageItem


@{
    ViewBag.Title = "Create";
}

<h2>Check in</h2>
<p>
    @{var today = DateTime.Now; }
</p>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @today
    
    <fieldset>
        <legend>GarageItem</legend>
        @Html.HiddenFor(model => model.Inchecked)
        <input type="hidden" id="Inchecked." value="@today" />

        <div class="editor-label">
            @Html.LabelFor(model => model.regNR)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.regNR)
            @Html.ValidationMessageFor(model => model.regNR)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Owner)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Owner)
            @Html.ValidationMessageFor(model => model.Owner)
        </div>

        <p>
            <input type="submit" value="Skapa" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
